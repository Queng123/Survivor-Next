{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/dashboard",
        "dest": "/index.js"
        },
        {
        "src": "/user/signin",
        "dest": "/index.js"
        },
        {
        "src": "/(.*)",
        "dest": "/index.js"
        }
    ]
}